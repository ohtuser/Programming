#include<bits/stdc++.h>
#define max 100
using namespace std;
vector<int> graph[max];
vector<bool> visited;
int n,m;

void dfs(int v) {
    visited[v] = true;
    cout << v << " ";
    for (int u : graph[v]) {
        if (!visited[u])
            dfs(u);
    }
}

int main(){
	cout << "Enter Number Of Node And Number Of Edges: ";
    cin >> n >> m;
    for(int i=0;i<m;i++){
        int x,y; cin >> x >> y;
        graph[x].push_back(y);
        graph[y].push_back(x);
    }
    visited.assign(n+1,false);
    cout << "Enter Source And Destination: ";
    int source, destination; cin >> source >> destination;
    dfs(source);
    if(visited[destination])
    	cout << endl << "Path Found";
    else
    	cout << endl << "Path Not Found";
}
